{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.fortunaa": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"set_block": "query.is_on_ground&&query.mark_variant==0"
						},
						{
							"set_blockk": "query.is_on_ground&&query.mark_variant==1"
						},
						{
							"set_blockkk": "query.is_on_ground&&query.mark_variant==2"
						},
						{
							"set_blockkkk": "query.is_on_ground&&query.mark_variant==3"
						}
					]
				},
				"set_block": {
					"on_entry": [
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:l1",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:l2",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:l3",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:l4",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:mine_l1_com1",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:mine_l1_com2",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:mine_l1_rare",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:mine_l2_com1",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:mine_l2_com2",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:mine_l3_com1",
						"/fill ~~-1~ ~~-1~ cr:oro 0 replace cr:mine_l3_com2"
					],
					"transitions": [
						{
							"default": "!q.is_on_ground"
						}
					]
				},
				"set_blockk": {
					"on_entry": [
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:l1",
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:l2",
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:l3",
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:mine_l1_com1",
