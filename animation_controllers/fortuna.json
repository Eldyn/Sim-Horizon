						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:mine_l1_rare",
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:mine_l2_com1",
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:mine_l2_com2",
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:mine_l3_com1",
						"/fill ~1~-1~1 ~-1~-1~-1 cr:oro 0 replace cr:mine_l3_com2"
					],
					"transitions": [
						{
							"default": "!q.is_on_ground"
						}
					]
				},
				"set_blockkk": {
					"on_entry": [
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:l1",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:l2",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:l3",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:mine_l1_com1",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:l4",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:mine_l1_com2",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:mine_l1_rare",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:mine_l2_com1",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:mine_l2_com2",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:mine_l3_com1",
						"/fill ~1~-1~1 ~-1~-2~-1 cr:oro 0 replace cr:mine_l3_com2"
					],
					"transitions": [
						{
							"default": "!q.is_on_ground"
						}
					]
				},
				"set_blockkkk": {
					"on_entry": [
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:l1",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:l2",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:l3",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:mine_l1_com1",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:l4",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:mine_l1_com2",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:mine_l1_rare",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:mine_l2_com1",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:mine_l2_com2",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:mine_l3_com1",
						"/fill ~2~-1~2 ~-2~-2~-2 cr:oro 0 replace cr:mine_l3_com2"
					],
					"transitions": [
						{
							"default": "!q.is_on_ground"
						}
					]
				}
			}
		}
	}
}